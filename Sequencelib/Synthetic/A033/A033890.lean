import Mathlib
import Sequencelib.Meta

open Synth

/-!
# A033890 sequence

## References

- T. Gauthier; and J. Urban (2023). Learning Program Synthesis for Integer Sequences from Scratch.
  The Thirty-Seventh AAAI Conference on Artificial Intelligence (AAAI-23)
-/

namespace Sequence

@[OEIS := A033890, offset := 0, maxIndex := 50, derive := true]
def A033890 (x : ℕ) : ℕ :=
  Int.toNat <| loop2 (λ (x y) ↦ x + y) (λ (x _y) ↦ x) (2 * (x + x)) 1 1

end Sequence