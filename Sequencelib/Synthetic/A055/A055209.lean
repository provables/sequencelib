import Mathlib
import Sequencelib.Meta

open Synth

/-!
# A055209 sequence
-/

namespace Sequence

@[OEIS := A055209, offset := 0, maxIndex := 31, derive := true]
def A055209 (x : ℕ) : ℕ :=
  Int.toNat <| loop (λ (x y) ↦ loop (λ (x y) ↦ (x * y) * y) y x) x 1

end Sequence