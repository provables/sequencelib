import Mathlib
import Sequencelib.Meta

open Synth

/-!
# A022099 sequence
-/

namespace Sequence

@[OEIS := A022099, offset := 0, maxIndex := 37, derive := true]
def A022099 (x : ℕ) : ℕ :=
  Int.toNat <| loop2 (λ (x y) ↦ x + y) (λ (x _y) ↦ x) x 1 (2 * 4)

end Sequence