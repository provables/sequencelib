import Mathlib
import Sequencelib.Meta

open Synth

/-!
# A121801 sequence
-/

namespace Sequence

@[OEIS := A121801, offset := 1, maxIndex := 25, derive := true]
def A121801 (n : ℕ) : ℕ :=
  let x := n - 1
  Int.toNat <| loop (λ (x y) ↦ loop2 (λ (x y) ↦ x + y) (λ (x _y) ↦ x) (y + y) 2 2 - x) x 0

end Sequence