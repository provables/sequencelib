import Mathlib
import Sequencelib.Meta

open Synth

/-!
# A081018 sequence

## References

- T. Gauthier; and J. Urban (2023). Learning Program Synthesis for Integer Sequences from Scratch.
  The Thirty-Seventh AAAI Conference on Artificial Intelligence (AAAI-23)
-/

namespace Sequence

@[OEIS := A081018, offset := 0, maxIndex := 25, derive := true]
def A081018 (x : ℕ) : ℕ :=
  Int.toNat <| loop (λ (x y) ↦ loop2 (λ (x y) ↦ y - x) (λ (x _y) ↦ x) (4 * y) 1 1 + x) x 0

end Sequence