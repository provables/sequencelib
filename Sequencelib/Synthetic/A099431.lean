import Mathlib
import Sequencelib.Meta

open Synth

/-!
# A099431 sequence
-/

namespace Sequence

@[OEIS := A099431, offset := 0, maxIndex := 30, derive := true]
def A099431 (x : ℕ) : ℕ :=
  Int.toNat <| loop2 (λ (_x y) ↦ y + y) (λ (x y) ↦ x + y) (x - 1) x 0

end Sequence