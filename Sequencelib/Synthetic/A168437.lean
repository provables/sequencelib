import Mathlib
import Sequencelib.Meta

open Synth

/-!
# A168437 sequence 
-/


namespace Sequence

@[OEIS := A168437, offset := 1, derive := true, maxIndex := 100]
def A168437 (n : ℕ) : ℕ :=
  let x := n - 1
  Int.toNat <| loop2 (λ (x y : ℤ) ↦ x + y) (λ (x _y : ℤ) ↦ x) 4 ((x % 2) + x) 1

end Sequence

