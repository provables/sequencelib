import Mathlib
import Sequencelib.Meta

open Synth

/-!
# A131438 sequence

## References

- T. Gauthier; and J. Urban (2023). Learning Program Synthesis for Integer Sequences from Scratch.
  The Thirty-Seventh AAAI Conference on Artificial Intelligence (AAAI-23)
-/

namespace Sequence

@[OEIS := A131438, offset := 1, maxIndex := 25, derive := true]
def A131438 (n : ℕ) : ℕ :=
  let x := n - 1
  Int.toNat <| loop (λ (x y) ↦ (2 * (x + y)) + y) x (x + x) + 1

end Sequence