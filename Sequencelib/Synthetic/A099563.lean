/-!

# A099563 sequence 
-/

namespace Sequence

@[OEIS := A099563, offset := 0]
def A099563 (n : ℕ) : ℤ :=
  let x := n - 0
  loop (λ(x y : ℤ) ↦ loop (λ(x y : ℤ) ↦ y) ((x / y)) (x)) (x) (x)


end Sequence
