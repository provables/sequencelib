import Mathlib
import Sequencelib.Meta

open Synth

/-!
# A058182 sequence
-/

namespace Sequence

@[OEIS := A058182, offset := 0, maxIndex := 14, derive := true]
def A058182 (x : ℕ) : ℕ :=
  Int.toNat <| loop2 (λ (_x y) ↦ y) (λ (x y) ↦ (y * y) + x) x 1 0

end Sequence