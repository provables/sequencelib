import Mathlib
import Sequencelib.Meta

open Synth

/-!
# A016947 sequence
-/

namespace Sequence

@[OEIS := A016947, offset := 0, maxIndex := 31, derive := true]
def A016947 (x : ℕ) : ℕ :=
