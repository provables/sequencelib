import Mathlib
import Sequencelib.Meta

open Synth

/-!
# A016812 sequence
-/

namespace Sequence

@[OEIS := A016812, offset := 0, maxIndex := 11, derive := true]
def A016812 (x : ℕ) : ℕ :=
  Int.toNat <| loop2 (λ (x y) ↦ (x * x) * y) (λ (x _y) ↦ x) 3 (2 * (x + x)) 1

end Sequence