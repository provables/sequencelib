import Mathlib
import Sequencelib.Meta

open Synth

/-!
# A122970 sequence
-/

namespace Sequence

@[OEIS := A122970, offset := 0, maxIndex := 100, derive := true]
def A122970 (x : ℕ) : ℕ :=
