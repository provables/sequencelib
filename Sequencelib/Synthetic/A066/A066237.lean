import Mathlib
import Sequencelib.Meta

open Synth

/-!
# A066237 sequence
-/

namespace Sequence

@[OEIS := A066237, offset := 1, maxIndex := 21, derive := true]
def A066237 (n : ℕ) : ℕ :=
  let x := n - 1
  Int.toNat <| loop (λ (x y) ↦ loop (λ (x y) ↦ x * y) y 2 + x) x 1

end Sequence