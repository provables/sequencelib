                {% for decl in lines[tag]["decls"]|sort %}
                <li><a href="{{ lines[tag].local_path }}#Sequence.{{ decl }}">{{ decl }}</a></li>
                {% endfor %}
            </ul>
        </li>
        {% endfor %}
    </ul>
</div>